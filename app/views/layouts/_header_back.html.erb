<div class="csp-header csp-header-back text-center">
  <div class="csp-header-buttons">
<% unless @header_buttons.nil? %>
    <!-- <a href="#"><span class="glyphicon glyphicon-menu-hamburger"></span></a> -->
    <% @header_buttons.each do |item| %>
    <a href="<%= item[:link] %>"><span class="glyphicon <%= item[:icon]%>"></span></a>
    <% end %>
<% end %>
<% if logged_in? %>
    <a href="/login"><span class="glyphicon glyphicon-edit"></span></a>
<% end %>
  </div>
  <!-- <h2 class="text-center"><a href="/">CSP GoLocal</a></h2> -->
  <a href="/"><img src="<%= asset_path('golocal-logo.png')%>" height="120" /></a>
</div>
