<h1>Editing Business Photo Gallery</h1>


<%= form_with(model: @business) do |form| %>
<div>
  <%= form.label :gallery, style: "display: block" %>
  <%= form.file_field :gallery, multiple: true %>
</div>

<div>
  <%= form.submit %>
</div>
<% end %>


<div>
  <%= link_to "Back to Edit business", edit_business_path(@business) %>
</div>
